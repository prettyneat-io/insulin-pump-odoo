# Odoo 18 Docker Environment Configuration
# Copy this to .env and modify as needed

# PostgreSQL Configuration
POSTGRES_DB=odoo
POSTGRES_USER=odoo
POSTGRES_PASSWORD=odoo

# Odoo Configuration  
ODOO_ADMIN_PASSWORD=admin

# Addons to install (comma-separated list)
# Include glucose_pumps_evercare to load demo data
INSTALL_ADDONS=base,stock,contacts,mail,website,glucose_pumps_evercare
