<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- 
            Stock Quants: Place each lot/serial number at the appropriate location with qty=1.
            - Available devices go to stock_location_available
            - Assigned devices go to stock_location_assigned
            - Scrapped devices go to stock.stock_location_scrapped
            - RMA devices go to stock_location_rma
        -->

        <!-- Available Tandem t:slim X2 Devices - in Available Devices location -->
        <record id="quant_tslim_001" model="stock.quant" forcecreate="1">
            <field name="product_id" search="[('product_tmpl_id', '=', ref('product_template_tslim_x2'))]" model="product.product"/>
            <field name="lot_id" ref="lot_tslim_001"/>
            <field name="location_id" ref="stock_location_available"/>
            <field name="quantity">1</field>
        </record>

